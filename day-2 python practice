# 15.--> multiplication table

 n=int(input("enter the value of n:"))
 print("multiplication table is:")
 for i in range(1,11):
     mt=n*i;
     print(mt," ")

# 16.--> fibonaci series

 n=int(input("enter the value of n:"))
 a=0
 b=1
 c=a+b
 for i in range(1,n):
     c=a+b
     print(c)
     a=b
     b=c
     c=a+b

# 17.--> sum of first n natural no.

 n=int(input("enter the limit:"))
 sum=0
 for  i in range(1,n+1):
     sum=sum+i

 print("Total sum is:",sum) 
